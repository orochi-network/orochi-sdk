{
  "name": "@orochi-network/sdk",
  "version": "1.0.18",
  "description": "Orochi Network SDK",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
