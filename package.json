  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.4",
    "crypto-js": "^4.2.0",
